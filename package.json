{
  "name": "@dazejs/jwt-provider",
  "version": "1.0.0",
  "description": "json web token service for dazejs",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist/*",
    "dev": "npm run clean && tsc -w",
    "build": "npm run clean && tsc",
    "test": "jest --verbose --runInBand --forceExit",
    "test:coverage": "jest --runInBand --forceExit --coverage",
    "example": "cross-env DAZE_ENV=prod DEBUG=jwt-provider:* nodemon --signal SIGTERM --exec ts-node example/index.ts --source-map --preserve-symlinks",
    "prepublish": "npm run build",
    "commitlint": "commitlint -e $GIT_PARAMS"
  },
  "keywords": [
    "jwt",
    "token",
    "jsonwebtoken",
    "dazejs"
  ],
  "author": "chan zewail",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@dazejs/framework": ">=3.0.0",
    "reflect-metadata": ">=0.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@dazejs/framework": "^3.0.0",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "reflect-metadata": "0.1.13",
    "commitlint": "^8.3.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  }
}
